                     </HBox.margin>
                  </JFXTextField>
               </children>
            </HBox>
            <JFXTreeTableView fx:id="treeView" currentItemsCount="1" editable="true" maxHeight="1.7976931348623157E308" style="-fx-font-size: 1.2em; -fx-font-family: Roboto; -fx-border-color: Black; -fx-alignment: Center; -fx-border-width: 1;" VBox.vgrow="ALWAYS" />
            <Text fx:id="txtCounter" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="0">
