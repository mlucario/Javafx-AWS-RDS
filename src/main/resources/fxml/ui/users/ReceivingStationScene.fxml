                  <Insets top="70.0" />
               </VBox.margin>
            </HBox>
            <JFXButton fx:id="btnSubmit" onAction="#submit" prefHeight="60.0" prefWidth="300.0" ripplerFill="#105c82" style="-fx-font-size: 2em; -fx-font-family: Roboto; -fx-font-weight: Bold; -fx-background-color: #2B98C6; -fx-background-radius: 100px;" text="Submit" textFill="WHITE">
               <VBox.margin>
                  <Insets top="40.0" />
