                  <JFXTextArea layoutX="20.0" layoutY="20.0" promptText="Enter Patern Barcode">
                     <font>
                        <Font name="Roboto Medium" size="15.0" />
                     </font>
                  </JFXTextArea>
               </children>
            </VBox>
         </children>
         <HBox.margin>
            <Insets bottom="50.0" top="50.0" />
