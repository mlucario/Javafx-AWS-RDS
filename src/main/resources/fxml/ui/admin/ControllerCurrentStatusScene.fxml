<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTabPane?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quy.controllers.admin.ManagementControllers">
   <children>
      <JFXTabPane HBox.hgrow="ALWAYS">
         <tabs>
            <Tab text="Inventory">
               <content>
                  <AnchorPane>
                     <children>
                        <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <VBox StackPane.alignment="TOP_LEFT">
                                 <children>
                                    <HBox alignment="CENTER_RIGHT">
                                       <children>
                                          <Label text="Search" textFill="#0011ff">
                                             <font>
                                                <Font name="Roboto" size="20.0" />
                                             </font>
                                          </Label>
                                          <JFXTextField fx:id="txtSN1" prefHeight="35.0" prefWidth="260.0" style="-fx-font-size: 1.3em;">
                                             <HBox.margin>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                             </HBox.margin>
                                          </JFXTextField>
                                       </children>
                                    </HBox>
                                    <JFXTreeTableView fx:id="tableViewInventory" VBox.vgrow="ALWAYS" />
                                    <HBox alignment="CENTER">
                                       <children>
                                          <VBox alignment="TOP_CENTER">
                                             <children>
                                                <Label text="Current Inventory" textFill="#1a00ff">
                                                   <font>
                                                      <Font name="Roboto Medium" size="22.0" />
                                                   </font>
                                                </Label>
                                                <Label fx:id="txtTotalInven" text="0" textFill="#2600ff">
                                                   <font>
                                                      <Font name="Roboto Medium" size="30.0" />
                                                   </font>
                                                </Label>
                                             </children>
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
                                             </HBox.margin>
                                          </VBox>
                                          <VBox alignment="TOP_CENTER" layoutX="233.0" layoutY="10.0">
                                             <children>
                                                <Label text="Current Passed" textFill="#1b7d2b">
                                                   <font>
                                                      <Font name="Roboto Medium" size="22.0" />
                                                   </font>
                                                </Label>
                                                <Label fx:id="txtPassed" text="0" textFill="#1b7d2b">
                                                   <font>
                                                      <Font name="Roboto Medium" size="30.0" />
                                                   </font>
                                                </Label>
                                             </children>
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
                                             </HBox.margin>
                                          </VBox>
                                          <VBox alignment="TOP_CENTER" layoutX="300.0" layoutY="10.0">
                                             <children>
                                                <Label text="Current Fail" textFill="RED">
                                                   <font>
                                                      <Font name="Roboto Medium" size="22.0" />
                                                   </font>
                                                </Label>
                                                <Label fx:id="txtfail" text="0" textFill="RED">
                                                   <font>
                                                      <Font name="Roboto Medium" size="30.0" />
                                                   </font>
                                                </Label>
                                             </children>
                                             <HBox.margin>
                                                <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
                                             </HBox.margin>
                                          </VBox>
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children>
                        </StackPane>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Shipped">
               <content>
                  <AnchorPane>
                     <children>
                        <StackPane layoutX="-6.0" layoutY="14.0" AnchorPane.bottomAnchor="-14.0" AnchorPane.leftAnchor="-6.0" AnchorPane.rightAnchor="6.0" AnchorPane.topAnchor="14.0">
                           <children>
                              <Label text="Tab 2" />
                           </children>
                        </StackPane>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
         </tabs>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </JFXTabPane>
   </children>
</HBox>
